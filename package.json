{
	"name": "irule",
	"displayName": "F5 Networks iRules",
	"description": "F5 Networks iRules Extension",
	"publisher": "bitwisecook",
	"version": "1.5.11",
	"keywords": [
		"f5",
		"f5networks",
		"irule",
		"irules",
		"remote development",
		"remote",
		"icr",
		"bigip",
		"big-ip",
		"icontrol",
		"icontrolrest"
	],
	"engines": {
		"vscode": "^1.35.0"
	},
	"bugs": {
		"url": "https://github.com/bitwisecook/vscode-iRule/issues"
	},
	"license": "SEE LICENSE IN LICENSE.txt",
	"icon": "images/f5.png",
	"galleryBanner": {
		"color": "#DCDCDC",
		"theme": "light"
	},
	"homepage": "https://github.com/bitwisecook/vscode-iRule/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/bitwisecook/vscode-iRule.git"
	},
	"categories": [
		"Formatters",
		"Other",
		"Programming Languages",
		"Snippets"
	],
	"activationEvents": [
		"onLanguage:irule-lang",
		"onCommand:icrfs.connect",
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": [
			{
				"title": "iRules",
				"properties": {
					"conf.irule-lang.diag.whenprio.enable": {
						"type": "boolean",
						"default": true,
						"description": "Enable/disable code diagnostic for 'when missing priority' diagnostic.",
						"scope": "workspace"
					},
					"conf.irule-lang.diag.doublesubst.enable": {
						"type": "boolean",
						"default": true,
						"description": "Enable/disable code diagnostic for 'double substition' diagnostic.",
						"scope": "workspace"
					},
					"conf.irule-lang.diag.arginject.enable": {
						"type": "boolean",
						"default": true,
						"description": "Enable/disable code diagnostic for 'argument injection' diagnostic.",
						"scope": "workspace"
					},
					"conf.icrfs.bigip.hosts": {
						"type": "array",
						"items": {
							"type": "string",
							"description": "BIG-IP Hostname",
							"pattern": "^([^@]+@)?[a-z0-9-.]+(:[0-9]+)?$"
						},
						"default": [
							"admin@192.168.1.245"
						],
						"description": "Specifies the hostnames icrfs has in its quick list."
					}
				}
			}
		],
		"commands": [
			{
				"command": "icrfs.connect",
				"title": "Connect and fetch iRules from the BIG-IP",
				"category": "IcrFS"
			},
			{
				"command": "icrfs.disconnect",
				"title": "Disconnect from the BIG-IP",
				"category": "IcrFS"
			},
			{
				"command": "icrfs.refresh",
				"title": "Refresh iRules from the BIG-IP",
				"category": "IcrFS"
			},
			{
				"command": "icrfs.settings",
				"title": "Settings",
				"category": "IcrFS"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "icrfs.connect",
					"when": "workbenchState != workspace"
				},
				{
					"command": "icrfs.refresh",
					"when": "workbenchState == workspace"
				}
			]
		},
		"languages": [
			{
				"id": "irule-lang",
				"aliases": [
					"irule",
					"iRule"
				],
				"extensions": [
					".irul",
					".irule",
					".tcl"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "irule-lang",
				"scopeName": "source.irule",
				"path": "./out/syntaxes/irule.tmLanguage"
			}
		],
		"snippets": [
			{
				"language": "irule-lang",
				"path": "./snippets/snippets.json"
			},
			{
				"language": "irule-lang",
				"path": "./snippets/iRuleSnippets.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./ && node out/build.js",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/chai": "^4.2.5",
		"@types/keytar": "^4.0.1",
		"@types/mocha": "^2.2.48",
		"@types/node": "^10.17.5",
		"@types/request": "^2.48.3",
		"chai": "^4.2.0",
		"converter": "0.0.5",
		"mocha": "^6.2.2",
		"nyc": "^14.1.1",
		"ts-node": "^8.5.2",
		"tslint": "^5.20.1",
		"typescript": "^3.7.2",
		"vscode": "^1.1.36"
	},
	"dependencies": {
		"request": "2.88.0"
	}
}
