    escape:
        match: '\\(\d{1,3}|x[a-fA-F0-9]+|u[a-fA-F0-9]{1,4}|.|\n)'
        name: constant.character.escape.irule
    inner-braces:
        begin: '\{'
        comment: 'matches a nested brace in a brace-enclosed word'
        end: '\}'
        patterns:
