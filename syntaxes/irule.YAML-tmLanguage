    numeric:
        match: '(?<![a-zA-Z])([0-7][0-7][0-7]|0x[0-9a-fA-F]|[+-]?([0-9]*[.])?[0-9]+f?)(?![\.a-zA-Z])'
        name: constant.numeric.irule
    operator:
        match: '(?<= |\d)(-|\+|~|starts_with|ends_with|contains|matches_regex|matches_glob|&|&&|and|\||\|\||or|<|<<|>|>>|\*|!|not|%|\/|<=|>=|==|eq|ne|equals|!=|\^)(?= |\d)'
        name: keyword.operator.irule
